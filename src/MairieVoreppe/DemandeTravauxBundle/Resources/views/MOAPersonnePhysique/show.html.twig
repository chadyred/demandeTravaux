{% extends 'MairieVoreppeDemandeTravauxBundle:MaitreOuvrage:layout.html.twig' %}

 {% block moa_content %}

 <div class="row">
    <div class="col-lg-12 table-responsive">
        <hr/>
        <caption>
            <h4>Un maitre d'ouvrage en tant que persone physique
                <br></h4>
            </caption>
            <table class="table table-bordered table-striped table-condensed">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <th>{{ entity.id }}</th>
                    </tr>
                    <tr>
                        <th>Nom</th>
                        <td>{{ entity.civil.nom }}</td>
                    </tr>
                    <tr>
                        <th>Prénom</th>
                        <td>{{ entity.civil.prenom }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
</div>

        <ul class="record_actions">
    <ul class="list-unstyled list-inline record_actions">
        <li>
            <a class="btn btn-info" href="{{ path('moapersonnephysique') }}">
            Retour à la liste des maîtres d'ouvrage en tant que personne physique
        </a>
    </li>
    <li>
        <a class="btn btn-success" href="{{ path('moapersonnephysique_edit', { 'id': entity.id }) }}">
            Edition
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
