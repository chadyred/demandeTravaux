{# Cette vue à pour but de listé l'ensemble des maitre d'ouvrage, c'est-à-dire physiques ou moraux #}
{% extends 'MairieVoreppeDemandeTravauxBundle:Recepisse:layout.html.twig' %}
{% block main_content %}


    {% block recepisse_content -%}

    <section class="col-lg-8 table-responsive">
        <hr/>
        <caption>
            <h1>Recepisse DT</h1>
        </caption>
        <table class="table table-bordered table-striped table-condensed">
            <tbody>
                <tr>
                   <th>Id</th>
                   <td>{{ entity.id }}</td>
               </tr>
                <tr>
                   <th>DT concerné</th>
                   <td><a target="_blank" href="{{ path('demandetravaux_show', {'id' : entity.dt.id} ) }}">{{ entity.dt.id }}</a></td>
               </tr>
               </tr>
                <tr>
                    <th>Générer récépissé</th>
                    <td><i class="fa fa-file-pdf-o"></i><a href="{{ path('recepissedt_generer_recepisse', {'id' : entity.id} ) }}">Télécharger</a></td>
                </tr>                   
                <tr>
                    <th>Prevoir investiguation</th>
                    <td>{{ entity.prevoirInvestiguation }}</td>
                </tr>
                <tr>
                    <th>Datecreation</th>
                    <td>{{ entity.dateCreation|date('Y-m-d H:i:s') }}</td>
                </tr>
                <tr>
                    <th>Extensionprevue</th>
                    <td>{{ entity.extensionPrevue }}</td>
                </tr>
                <tr>
                    <th>Modificationencours</th>
                    <td>{{ entity.modificationEnCours }}</td>
                </tr>
                <tr>
                    <th>Nomrepresentant</th>
                    <td>{{ entity.nomRepresentant }}</td>
                </tr>
                <tr>
                    <th>Telephonerepresentant</th>
                    <td>{{ entity.telephoneRepresentant }}</td>
                </tr>
                <tr>
                    <th>Planjoint</th>
                    <td>{{ entity.planJoint }}</td>
                </tr>
                <tr>
                    <th>Prendreencompteservitude</th>
                    <td>{{ entity.prendreEnCompteServitude }}</td>
                </tr>
                <tr>
                    <th>Branchementrattache</th>
                    <td>{{ entity.branchementRattache }}</td>
                </tr>
                <tr>
                    <th>Recommandationsecurite</th>
                    <td>{{ entity.recommandationSecurite }}</td>
                </tr>
                <tr>
                    <th>Rubriqueguidetechsecurite</th>
                    <td>{{ entity.rubriqueGuideTechSecurite }}</td>
                </tr>
                <tr>
                    <th>Mesuresecurite</th>
                    <td>{{ entity.mesureSecurite }}</td>
                </tr>
                <tr>
                    <th>Telservicedegradation</th>
                    <td>{{ entity.telServiceDegradation }}</td>
                </tr>
                <tr>
                    <th>Servicedepartementincendiesecours</th>
                    <td>{{ entity.serviceDepartementIncendieSecours }}</td>
                </tr>
                <tr>
                    <th>Telservicedepartementincendiesecours</th>
                    <td>{{ entity.telServiceDepartementIncendieSecours }}</td>
                </tr>
                <tr>
                    <th>Responsabledossier</th>
                    <td>{{ entity.responsableDossier }}</td>
                </tr>
                <tr>
                    <th>Telresponsabledossier</th>
                    <td>{{ entity.telResponsableDossier }}</td>
                </tr>
            </tbody>
        </table>

            <ul class="list-unstyled list-inline">
        <li>
            <a class="btn btn-info" href="{{ path('recepissedt') }}">
                Retour à la liste des récepissés de DT
            </a>
        </li>
        <li>
            <a class="btn btn-success" href="{{ path('recepissedt_edit', { 'id': entity.id }) }}">
                Modifier
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
    {% endblock %}
{% endblock %}
