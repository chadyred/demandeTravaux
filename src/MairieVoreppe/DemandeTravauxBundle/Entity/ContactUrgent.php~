<?php

namespace MairieVoreppe\DemandeTravauxBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use MairieVoreppe\DemandeTravauxBundle\Model\PersonnePhysique;

/**
 * ContactUrgent
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="MairieVoreppe\DemandeTravauxBundle\Entity\ContactUrgentRepository")
 */
class ContactUrgent extends PersonnePhysique
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;
    
    
    /**
     * @var \Doctrine\Common\Collections\ArrayCollection() 
     * 
     * Une personne n'a qu'une contactUrgent. Elle sera remplit et complÃ©tÃ©e lors de la crÃ©ation de celui-ci.
     * 
     * @ORM\OneToMany(targetEntity="MairieVoreppe\DemandeTravauxBundle\Model\Travaux", cascade={"persist", "remove"}, mappedBy="contactsUrgent")
     */
    protected $travaux; 


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }


    /**
     * Set travaux
     *
     * @param \MairieVoreppe\DemandeTravauxBundle\Model\Travaux $travaux
     * @return ContactUrgent
     */
    public function setTravaux(\MairieVoreppe\DemandeTravauxBundle\Model\Travaux $travaux = null)
    {
        $this->travaux = $travaux;

        return $this;
    }

    /**
     * Get travaux
     *
     * @return \MairieVoreppe\DemandeTravauxBundle\Model\Travaux 
     */
    public function getTravaux()
    {
        return $this->travaux;
    }

    /**
     * Set civilite
     *
     * @param \MairieVoreppe\DemandeTravauxBundle\Entity\Civilite $civilite
     * @return ContactUrgent
     */
    public function setCivilite(\MairieVoreppe\DemandeTravauxBundle\Entity\Civilite $civilite = null)
    {
        $this->civilite = $civilite;

        return $this;
    }

    /**
     * Get civilite
     *
     * @return \MairieVoreppe\DemandeTravauxBundle\Entity\Civilite 
     */
    public function getCivilite()
    {
        return $this->civilite;
    }

    /**
     * Set adresse
     *
     * @param \MairieVoreppe\DemandeTravauxBundle\Entity\Adresse $adresse
     * @return ContactUrgent
     */
    public function setAdresse(\MairieVoreppe\DemandeTravauxBundle\Entity\Adresse $adresse = null)
    {
        $this->adresse = $adresse;

        return $this;
    }

    /**
     * Get adresse
     *
     * @return \MairieVoreppe\DemandeTravauxBundle\Entity\Adresse 
     */
    public function getAdresse()
    {
        return $this->adresse;
    }
}
